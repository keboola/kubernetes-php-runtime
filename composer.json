{
    "name": "kubernetes/php-runtime",
    "description": "Runtime for kubernetes/php-client",
    "license": "MIT",
    "keywords": [
        "kubernetes",
        "docker",
        "runtime"
    ],
    "authors": [
        {
            "name": "Allan Sun",
            "email": "allan.sun@bricre.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6|^7",
        "phpdocumentor/reflection-docblock": "^3.0|^4.0|^5.0",
        "psr/log": "^3.0",
        "symfony/property-info": "^5|^6"
    },
    "require-dev": {
        "phpunit/phpunit": "~9"
    },
    "config": {
        "sort-packages": true
    },
    "conflict": {
        "phpdocumentor/reflection-docblock": "<3.2.2"
    },
    "autoload": {
        "psr-4": {
            "KubernetesRuntime\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KubernetesRuntime\\Tests\\": "tests"
        }
    },
    "prefer-stable": true
}
